#include <iostream>
                    using namespace std;
                    template <typename T>
                    T Max(T a, T b) 
                    {
                             return (a > b) ? a : b;
                        }
                  int main() {
                               cout <<"Max of 10 and 20 is: "<<Max(10, 20)<<endl;          // int
                               cout <<"Max of 5.5 and 3.2 is: "<<Max(5.5, 3.2)<<endl;      // double
                               cout <<"Max of 'A' and 'Z' is: "<<Max('A', 'Z')<<endl;      // char
                    return 0;
                     }
