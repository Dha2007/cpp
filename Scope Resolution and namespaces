#include <iostream>
using namespace std;
int num = 50;
namespace Example {
    int num = 100;
    void display() {
        cout << "Inside namespace Example: num = " << num << endl;
    }
}
int main() {
    int num = 10;
    cout << "Local num: " << num << endl;
    cout << "Global num: " << ::num << endl;
    cout << "Namespace num: " << Example::num << endl;
    Example::display();
    return 0;
}
